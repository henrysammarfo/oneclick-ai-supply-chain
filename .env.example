# Copy this to .env and fill in your keys
# ============================================

# === CORE LLM APIS ===
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_OPENAI_HOST=chat-gpt26.p.rapidapi.com
RAPIDAPI_PROVIDER=chat-gpt26
RAPIDAPI_LLM_MODEL=GPT-5-mini
OPENAI_API_KEY=your_openai_key_here
OPENAI_MODEL=gpt-4o
GOOGLE_API_KEY=your_google_api_key_here

# === REAL-TIME SERVICES ===
GEOCODING_API_KEY=your_opencage_key_here
EXCHANGE_RATE_API_KEY=your_exchangerate_key_here

# === DISCOVERY & SEARCH APIS ===
GOOGLE_SEARCH_API_KEY=your_google_search_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# === SYSTEM CONFIGURATION ===
REGISTRY_DB=sqlite:///agent_registry.db
HOST=0.0.0.0
PORT=8000
WEBSOCKET_PORT=8001
MESSAGE_FEED_PATH=logs/agent_feed.jsonl
SUPPLY_GRAPH_PATH=supply_graph.json

# === FEATURE FLAGS ===
OFFLINE_MODE=0
ENABLE_GOOGLE_SEARCH=0
USE_REAL_DISCOVERY=true
DEMO_MODE=production
LOG_LEVEL=INFO
ENABLE_CACHING=true

# === RATE LIMITS (for demo) ===
MAX_SUPPLIERS_PER_SEARCH=15
MAX_COMPONENTS_PER_PRODUCT=50
NEGOTIATION_ROUNDS=3

