      "command": "cmd",
      "args": ["/c", "npx", "-y", "@automatalabs/mcp-server-playwright"]
    },
    "executeautomation-playwright-server": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@executeautomation/playwright-mcp-server"]
    },
    "playwright-server": {
      "command": "cmd",
      "args": ["/c", "npx", "@playwright/mcp@latest"]
